{
    "name": "@ansible/ansible-ui",
    "version": "0.0.117",
    "type": "module",
    "scripts": {
        "start": "concurrently npm:frontend npm:backend --prefix-colors cyan,green",
        "frontend": "webpack serve --mode development",
        "backend": "NODE_ENV=development PORT=3001 nodemon --watch './backend/**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only' backend/main.ts | pino-zen",
        "test": "concurrently --kill-others-on-fail npm:tsc npm:eslint npm:prettier npm:cypress --prefix-colors cyan,green,blue,magenta,gray",
        "build": "concurrently --kill-others-on-fail npm:frontend:build npm:backend:build --prefix-colors cyan,green,blue,magenta,gray",
        "frontend:build": "webpack --mode production --env pwa",
        "backend:build": "npm run clean && rollup --config rollup.config.ts --configPlugin typescript",
        "clean": "rm -rf build coverage .nyc_output",
        "tsc": "tsc --noEmit",
        "eslint": "eslint --max-warnings=0 frontend backend framework cypress",
        "eslint:fix": "eslint --fix frontend backend framework cypress",
        "prettier": "prettier --check frontend backend framework cypress",
        "prettier:fix": "prettier --write frontend backend framework cypress",
        "upgrade": "npx npm-check-updates '/^@patternfly.*$/' --upgrade --target latest --doctor && npx npm-check-updates '/^(?!@patternfly).*$/' --upgrade --target minor --doctor && npm audit fix && npm dedup",
        "postinstall": "npm run certs",
        "certs": "mkdir -p certs && openssl req -subj '/C=US' -new -newkey rsa:2048 -sha256 -days 365 -nodes -x509 -keyout certs/tls.key -out certs/tls.crt",
        "docker:build": "docker build --tag ansible/console .",
        "docker:run": "npm run docker:build && docker run --rm -e LOG_LEVEL=debug -p 3001:3001 -e PORT=3001 -v $PWD/certs:/home/node/certs ansible/console| pino-zen",
        "cypress": "npm run clean && concurrently npm:cypress:frontend npm:cypress:run --prefix-colors cyan,green,blue --kill-others --success first",
        "cypress:run": "wait-on -d 5000 -i 1000 https-get://localhost:3002 && cypress run --env mock=true --config video=false",
        "cypress:open": "cypress open --env mock=true --e2e --browser chrome",
        "cypress:frontend": "webpack serve --mode production --no-open --no-client-overlay",
        "coverage": "open coverage/lcov-report/index.html",
        "coverage:check": "npx nyc report --check-coverage --statements 38 --branches 32 --functions 32 --lines 40 --report-dir ./coverage --temp-dir .nyc_output --reporter=text-summary --exclude-after-remap false",
        "prepare": "husky install",
        "prepush": "concurrently --kill-others-on-fail npm:tsc npm:eslint npm:prettier --prefix-colors cyan,green,blue,magenta,gray",
        "i18n": "npx i18next-parser --config i18next-parser.config.cjs"
    },
    "dependencies": {
        "@hookform/resolvers": "2.9.8",
        "@patternfly/patternfly": "4.215.1",
        "@patternfly/react-charts": "6.92.0",
        "@patternfly/react-core": "4.239.0",
        "@patternfly/react-icons": "4.90.0",
        "@patternfly/react-table": "4.108.0",
        "@react-hook/resize-observer": "1.2.6",
        "@sinclair/typebox": "0.24.44",
        "@types/cookie": "0.5.1",
        "@types/debounce": "1.2.1",
        "@types/json-schema": "7.0.11",
        "@types/luxon": "3.0.1",
        "@types/node": "18.8.3",
        "@types/react": "17.0.50",
        "@types/react-dom": "17.0.17",
        "@types/react-router-dom": "5.3.3",
        "cookie": "0.5.0",
        "debounce": "1.2.1",
        "i18next-browser-languagedetector": "^6.1.6",
        "i18next-http-backend": "^1.4.4",
        "ky": "0.31.3",
        "luxon": "3.0.4",
        "p-limit": "4.0.0",
        "pino": "8.6.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "7.37.0",
        "react-i18next": "^11.18.6",
        "react-router-dom": "6.4.2",
        "swr": "1.3.0",
        "type-fest": "2.19.0"
    },
    "devDependencies": {
        "@babel/core": "7.19.3",
        "@babel/preset-react": "7.18.6",
        "@babel/preset-typescript": "7.18.6",
        "@cypress/code-coverage": "3.10.0",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
        "@rollup/plugin-commonjs": "22.0.2",
        "@rollup/plugin-node-resolve": "14.1.0",
        "@rollup/plugin-replace": "4.0.0",
        "@rollup/plugin-strip": "2.1.0",
        "@rollup/plugin-typescript": "8.5.0",
        "@svgr/webpack": "6.4.0",
        "@types/css-minimizer-webpack-plugin": "3.2.0",
        "@types/html-webpack-plugin": "3.2.6",
        "@types/mini-css-extract-plugin": "2.5.0",
        "@types/webpack": "5.28.0",
        "@types/webpack-dev-server": "4.7.1",
        "@typescript-eslint/eslint-plugin": "5.39.0",
        "@typescript-eslint/parser": "5.39.0",
        "axe-core": "4.4.3",
        "babel-loader": "8.2.5",
        "babel-plugin-istanbul": "6.1.1",
        "babel-plugin-transform-class-properties": "6.24.1",
        "concurrently": "7.4.0",
        "copy-webpack-plugin": "11.0.0",
        "coverage-istanbul-loader": "3.0.5",
        "css-loader": "6.7.1",
        "css-minimizer-webpack-plugin": "4.2.1",
        "cypress": "10.9.0",
        "cypress-axe": "1.0.0",
        "eslint": "8.24.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-i18next": "^6.0.0-4",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.31.8",
        "eslint-plugin-react-hooks": "4.6.0",
        "html-webpack-plugin": "5.5.0",
        "husky": "^8.0.1",
        "merge-jsons-webpack-plugin": "^2.0.1",
        "mini-css-extract-plugin": "2.6.1",
        "nodemon": "2.0.20",
        "pino-zen": "2.0.7",
        "prettier": "2.7.1",
        "react-refresh": "0.14.0",
        "rollup": "2.79.1",
        "style-loader": "3.3.1",
        "ts-node": "10.9.1",
        "typescript": "4.8.4",
        "wait-on": "6.0.1",
        "webpack": "5.74.0",
        "webpack-cli": "4.10.0",
        "webpack-dev-server": "4.11.1",
        "workbox-webpack-plugin": "6.5.4"
    },
    "nyc": {
        "all": true,
        "include": [
            "../frontend/src/**/*.ts",
            "../frontend/src/**/*.tsx"
        ]
    }
}
