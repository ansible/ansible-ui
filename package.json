  "nyc": {
    "all": true,
    "include": [
      "../frontend/src/**/*.ts",
      "../frontend/src/**/*.tsx"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
