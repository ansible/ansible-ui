    "react-refresh": "0.14.0",
    "rollup": "3.9.1",
    "style-loader": "3.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "typescript-plugin-styled-components": "2.0.0",
    "wait-on": "6.0.1",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "workbox-webpack-plugin": "6.5.4"
  },
  "nyc": {
    "all": true,
    "include": [
      "../frontend/src/**/*.ts",
      "../frontend/src/**/*.tsx"
    ]
  }
}
