{
  "name": "@ansible/ansible-ui",
  "description": "Ansible Automation Platform UI",
  "version": "0.0.425",
  "author": "Red Hat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ansible/ansible-ui.git"
  },
  "homepage": "https://github.com/ansible/ansible-ui#readme",
  "type": "module",
  "scripts": {
    "start": "concurrently npm:frontend npm:proxy --prefix-colors cyan,green",
    "frontend": "webpack serve --mode development",
    "proxy": "NODE_ENV=development PORT=3001 nodemon --watch './proxy/**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only' proxy/main.ts | pino-zen",
    "test": "concurrently --kill-others-on-fail npm:tsc npm:eslint npm:prettier npm:cypress --prefix-colors cyan,green,blue,magenta,gray",
    "build": "concurrently --kill-others-on-fail npm:frontend:build npm:proxy:build --prefix-colors cyan,green,blue,magenta,gray",
    "frontend:build": "webpack --mode production --env pwa",
    "proxy:build": "npm run clean && rollup --config rollup.config.ts --configPlugin typescript",
    "clean": "rm -rf build coverage .nyc_output",
    "tsc": "tsc --noEmit",
    "eslint": "eslint --max-warnings=0 frontend proxy framework cypress",
    "eslint:fix": "eslint --fix frontend proxy framework cypress",
    "prettier": "prettier --check !**/*.scss frontend proxy framework cypress",
    "prettier:fix": "prettier --write frontend proxy framework cypress",
    "upgrade": "npx npm-check-updates '/^@patternfly.*$/' --upgrade --target latest --doctor && npx npm-check-updates '/^(?!@patternfly).*$/' --upgrade --target minor --doctor && npm audit fix || true && npm dedup || true ",
    "docker:build": "docker build --tag ansible/console .",
    "docker:run": "npm run docker:build && docker run --rm -e LOG_LEVEL=debug -p 3001:3001 -e PORT=3001 -v $PWD/certs:/home/node/certs ansible/console| pino-zen",
    "cypress": "npm run clean && concurrently npm:cypress:frontend npm:cypress:run --prefix-colors cyan,blue --kill-others --success first",
    "cypress:run": "wait-on -i 1000 https-get://localhost:3002 && cypress run --config video=false",
    "cypress:run:live": "wait-on -i 1000 https-get://localhost:3002 && cypress run --config video=false --env server=https://localhost:8043,username=admin,password=password",
    "cypress:open": "cypress open --e2e --browser chrome",
    "cypress:open:live": "cypress open --env server=https://localhost:8043,username=admin,password=password --e2e --browser chrome",
    "cypress:frontend": "webpack serve --mode production --no-open --no-client-overlay",
    "coverage": "open coverage/lcov-report/index.html",
    "coverage:check": "npx nyc report --check-coverage --statements 20 --branches 20 --functions 20 --lines 20 --report-dir ./coverage --temp-dir .nyc_output --reporter=text-summary --exclude-after-remap false",
    "prepare": "husky install",
    "prepush": "concurrently --kill-others-on-fail npm:tsc npm:eslint npm:prettier --prefix-colors cyan,green,blue,magenta,gray",
    "i18n": "npx i18next-parser --config i18next-parser.config.cjs",
    "build:mjs": "npx tsc -p tsconfig.mjs.json",
    "build:cjs": "npx tsc -p tsconfig.cjs.json",
    "gendocs": "./scripts/gendocs.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@patternfly/patternfly": "4.222.4",
    "@patternfly/react-charts": "6.94.15",
    "@patternfly/react-core": "4.267.6",
    "@patternfly/react-icons": "4.93.3",
    "@patternfly/react-table": "4.112.6",
    "@react-hook/resize-observer": "1.2.6",
    "@sinclair/typebox": "0.25.16",
    "@types/cookie": "0.5.1",
    "@types/debounce": "1.2.1",
    "@types/etag": "1.8.1",
    "@types/get-value": "3.0.3",
    "@types/json-schema": "7.0.11",
    "@types/luxon": "3.2.0",
    "@types/node": "18.11.18",
    "@types/react": "17.0.52",
    "@types/react-dom": "17.0.18",
    "@types/react-router-dom": "5.3.3",
    "cookie": "0.5.0",
    "debounce": "1.2.1",
    "etag": "1.8.1",
    "get-value": "3.0.1",
    "i18next-browser-languagedetector": "7.0.1",
    "i18next-http-backend": "2.1.1",
    "ky": "0.33.1",
    "luxon": "3.2.0",
    "p-limit": "4.0.0",
    "pino": "8.8.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.41.3",
    "react-i18next": "11.18.6",
    "react-router-dom": "6.6.1",
    "selfsigned": "2.1.1",
    "styled-components": "5.3.6",
    "swr": "1.3.0",
    "type-fest": "3.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@cypress/code-coverage": "3.10.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@rollup/plugin-commonjs": "23.0.7",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-strip": "3.0.2",
    "@rollup/plugin-typescript": "10.0.1",
    "@svgr/webpack": "6.5.1",
    "@types/css-minimizer-webpack-plugin": "3.2.0",
    "@types/html-webpack-plugin": "3.2.6",
    "@types/mini-css-extract-plugin": "2.5.0",
    "@types/styled-components": "5.1.26",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-server": "4.7.1",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "babel-loader": "9.1.0",
    "babel-plugin-istanbul": "6.1.1",
    "babel-plugin-styled-components": "2.0.7",
    "babel-plugin-transform-class-properties": "6.24.1",
    "concurrently": "7.6.0",
    "copy-webpack-plugin": "11.0.0",
    "coverage-istanbul-loader": "3.0.5",
    "css-loader": "6.7.3",
    "css-minimizer-webpack-plugin": "4.2.2",
    "cypress": "11.2.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-i18next": "6.0.0-6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.2",
